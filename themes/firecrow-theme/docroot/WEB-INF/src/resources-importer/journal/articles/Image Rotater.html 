<script type="text/javascript" charset="utf-8">

AUI().ready('aui-carousel', function(A) {

	var component = new A.Carousel(
		{
			intervalTime: 2.5,
			contentBox: '#demo',
			activeIndex: 0,
			height: 254,
			width: 940
		}
	).render();
});

</script>